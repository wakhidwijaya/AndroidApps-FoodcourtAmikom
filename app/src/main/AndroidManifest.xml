<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.foodcourt">

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_restaurant_menu_black_24dp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".feature.customer.table.number.InputNumberActivity"></activity>
        <activity android:name=".feature.customer.table.TableNumberActivity" />
        <activity android:name=".feature.core.login.RegisterCustomers" />
        <activity android:name=".feature.order.active.OrderActiveActivity" />
        <activity android:name=".feature.customer.payment.OrderCustomerActivity" />
        <activity android:name=".feature.customer.restaurants.menu.MenuCustomerActivity" />
        <activity android:name=".feature.customer.restaurants.RestaurantListActivity" />
        <activity android:name=".feature.customer.table.qrcode.QrCustomerActivity" />
        <activity android:name=".feature.customer.dashboard.DashboardCustomerActivity" />
        <activity android:name=".feature.core.login.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>